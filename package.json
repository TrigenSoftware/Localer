{
  "name": "localer",
  "version": "2.0.0",
  "description": "Locales collector from js sources.",
  "main": "lib/lib.js",
  "scripts": {
    "test": "mocha -b test/locales.js",
    "compile": "babel ./src -d ./lib",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/Localer.git"
  },
  "keywords": [
    "i18n",
    "i18n-node",
    "i18n-for-browser",
    "locales"
  ],
  "author": "dangreen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrigenSoftware/Localer/issues"
  },
  "homepage": "https://github.com/TrigenSoftware/Localer#readme",
  "dependencies": {
    "ansi-to-html": "^0.4.1",
    "argue-cli": "^1.1.1",
    "babel-code-frame": "^6.11.0",
    "babel-polyfill": "^6.13.0",
    "babel-traverse": "^6.15.0",
    "babylon": "^6.9.2",
    "colour": "^0.7.1",
    "escape-html": "^1.0.3",
    "find-rc": "^3.0.1",
    "glob": "^7.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "mocha": "^3.0.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "bin": {
    "localer": "./lib/bin.js"
  }
}
